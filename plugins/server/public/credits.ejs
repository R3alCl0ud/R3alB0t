<!DOCTYPE html>
<html lang="en">
<head>
    <%- include templates/head %>
    <title>Credits - <%= guild %></title>
</head>
<body onload="loaded()">
	<%- include templates/navbar {user: user} %>
    <%- include templates/terminal-top {guild: guild, icon: icon} %>
    <div class="container">


        <div class="col-md-12">
            <div class="list-group">
                <% for(var i=0; i <credits.credits.length; i++) { %>
                    <% var member = {
                        name: credits.names[i],
                        descriminator: credits.descriminators[i],
                        avatar: credits.avatars[i],
                        credits: credits.credits[i]
                    } %>
                    <%- include templates/creditAccount {member} %>
                <% } %>
            </div>
        </div>
    </div>
</body>
